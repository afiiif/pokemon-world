{
  "name": "pokemon-world",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "prebuild": "yarn prepare-data",
    "build": "next build",
    "postbuild": "next-sitemap",
    "start": "next start",
    "export": "next build && next export",
    "prepare": "husky install",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:types": "tsc --noEmit",
    "code-check": "yarn format:fix && yarn lint && yarn lint:types",
    "test:e2e": "playwright test",
    "codegen": "graphql-codegen",
    "generate:pokemons": "tsc scripts/pokemons.ts --outDir .temp && node .temp/scripts/pokemons.js",
    "generate:statistics": "tsc scripts/statistics.ts --outDir .temp && node .temp/scripts/statistics.js",
    "prepare-data": "yarn generate:pokemons && yarn generate:statistics"
  },
  "dependencies": {
    "@headlessui/react": "^1.6.6",
    "@nivo/chord": "^0.79.1",
    "@nivo/core": "^0.79.0",
    "clsx": "^1.2.1",
    "graphql": "^16.6.0",
    "graphql-request": "^4.3.0",
    "mini-svg-data-uri": "^1.4.4",
    "next": "12.2.5",
    "next-pwa": "^5.6.0",
    "next-seo": "^5.5.0",
    "next-sitemap": "^3.1.21",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^4.4.0",
    "react-masonry-css": "^1.0.16",
    "react-power-ups": "^2.0.0",
    "react-query": "^4.0.0-beta.23",
    "react-slick": "^0.29.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.1.0",
    "@graphql-codegen/cli": "^2.11.6",
    "@graphql-codegen/typescript": "^2.7.3",
    "@graphql-codegen/typescript-operations": "^2.5.3",
    "@playwright/test": "^1.25.1",
    "@tailwindcss/nesting": "^0.0.0-insiders.565cd3e",
    "@types/node": "18.7.11",
    "@types/react": "18.0.17",
    "@types/react-dom": "18.0.6",
    "@types/react-slick": "^0.23.10",
    "@typescript-eslint/eslint-plugin": "^5.34.0",
    "@typescript-eslint/parser": "^5.34.0",
    "autoprefixer": "^10.4.8",
    "eslint": "^8.22.0",
    "eslint-config-next": "^12.2.5",
    "eslint-config-react-moonstone": "^1.3.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "postcss": "^8.4.16",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "tailwindcss": "^3.1.8",
    "typescript": "4.7.4"
  }
}
